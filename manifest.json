{
    "name": "__MSG_extName__",
    "description": "__MSG_extDescription__",
    "version": "1.1",
    "manifest_version": 2,
    "default_locale": "en",
    "options_page": "options.html",
    "icons": {
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background" : {
      "scripts": [
        "src/debug.js",
        "src/common.js",
        "src/common_func.js",
        "src/main.js"
      ]
    },
    "content_scripts": [
      {
        "matches": [ "http://*/*", "https://*/*" ],
        "js": [
          "src/common.js",
          "src/common_func.js",
          "src/content_scripts/openDifferentDomain.js"
        ]
      }
    ],
    "permissions": [
      "tabs", "storage"
    ]
}
